jira --action renderRequest --file "target/output/jiracliuser/renderRequestViewUserAfterRemove.txt" --request "/secure/admin/user/ViewUser.jspa" --requestParameters "name=testuser1"
jira --action renderRequest --issue "ZJIRACLI-3" --file "target/output/jiracli/renderRequest.txt"
jira --action renderRequest --project "zjiracli" --file "target/output/jiracli/verifyProjectIssueTypeScreenScheme.txt" --request "plugins/servlet/project-config/ZJIRACLI/summary"
jira --action renderRequest --project "zjiracli" --file "target/output/jiracli/verifyUpdateProjectIssueTypeScreenSchemeDefault.txt" --request "plugins/servlet/project-config/ZJIRACLI/summary"
jira --action renderRequest --request "/rest/project-templates/1.0/templates" --type "FORM_URL_ENCODED" --acceptType "JSON" --verbose

# run
