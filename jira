# jira-cli examples
java -jar jira-cli-5.7.0.jar --server https://jira.examplegear.com --action addVote --issue "ZSUB-1" --user "admin" --password ""
java -jar jira-cli-5.7.0.jar --server https://jira.examplegear.com --action getProject --project "ZPROJECT" --user "user" --password ""
java -jar jira-cli-5.7.0.jar --server https://jira.examplegear.com --action getProjectList --regex "ZPROJECT" --outputFormat 999 --user "user" --password ""
java -jar jira-cli-5.7.0.jar --server https://jira.examplegear.com --action run --user "anon" --password ""

