# boxcar
function boxcar3(A::AbstractArray)
  out = similar(A)
    R = CartesianIndices(A)
    IFirst, Ilast = first(R), last(R)
    I1 = oneunit(Ifirst)
    for I in R
      n, s = zero(eltype(out))
      for J in max(Ifirst,I-I1):min(Ilast,I+I1)
        s += A[J]
        n += 1
      end
  out[I] = s/n
    end
    out
end

